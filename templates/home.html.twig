{% extends "base.html.twig" %} {% block body %}
<div class="message">{{ message }}</div>
<div class='containter_global'>
    <div class="container bcontent">
        <h2>Liste des utilisateur </h2>
        <hr /> {% for items in users %}
        <div class="media border p-3">
            <img class="mr-3" src="templates/images/defaultimg.png" alt="Suresh Dasari" style="width:80px;">
            <div class="media-body">
                <h5> {{items.name }}{{ " " }} {{items.lastname }} <small><i>{{items.birth}}</i></small></h5>
                <p>{{items.mail}}</p>
            </div>
        </div>
        {% endfor %}
    </div>
    <div class="container bcontent">
        <h2>Liste des voitures </h2>
        <hr /> {% for items in cars %}
        <div class="media border p-3">
            <img class="mr-3" src="templates/images/car.png" alt="Suresh Dasari" style="width:80px;">
            <div class="media-body">
                <h5> {{items.brand }}{{ " " }} {{items.model }} <small><i>{{items.color}}</i></small></h5>
                <p>Nombre de place :{{ " " }} {{items.nbrSlots}}</p>
            </div>
        </div>
        {% endfor %}
    </div>
</div> {% endblock %}